{
  "name": "savr-web-application",
  "version": "1.0.0",
  "description": "Save anything from the web",
  "main": "index.js",
  "scripts": {
    "build:dev": "NODE_ENV=development npx webpack --config configurations/webpack/development.js",
    "build:prod": "NODE_ENV=production npx webpack --config configurations/webpack/production.js",
    "clear-cache": "npm rimraf ./node_modules/.cache/hard-source",
    "clear-everything": "npx cache clean && npx rimraf ./node_modules",
    "fresh-start": "npm run clear-everything && npm install && npm run start",
    "lint": "npx eslint src/ --ext .js,.jsx",
    "precommit-check": "npm run lint",
    "start": "NODE_ENV=development npx webpack-dev-server --config configurations/webpack/development.js --watch --open",
    "test": "npx jest src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/savr-web-application.git"
  },
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/savr-web-application/issues"
  },
  "homepage": "https://github.com/jaebradley/savr-web-application#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@babel/polyfill": "^7.8.7",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "apollo-boost": "^0.4.7",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.7.0",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.3",
    "react-router": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-build-notifier": "^2.0.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
